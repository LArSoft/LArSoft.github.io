           art::Ptr&lt;raw::RawDigit&gt; &#38;rawdigit);
      Wire(RegionsOfInterest_t&#38;&#38; sigROIlist,
           art::Ptr&lt;raw::RawDigit&gt; &#38;rawdigit);

      <span class="comment">// Get Methods</span>
      <span class="comment">// zero-padded full length vector filled with ROIs</span>
      std::vector&lt;<span class="predefined-type">float</span>&gt;  Signal() <span class="directive">const</span>;

      <span class="directive">const</span> RegionsOfInterest_t&#38; SignalROI()  <span class="directive">const</span>;
      size_t                     NSignal()    <span class="directive">const</span>;
      art::Ptr&lt;raw::RawDigit&gt;    RawDigit()   <span class="directive">const</span>;
      geo::View_t                View()       <span class="directive">const</span>;
      geo::SigType_t             SignalType() <span class="directive">const</span>;
      uint32_t                   Channel()    <span class="directive">const</span>;
    };
